$(document).ready(function(){
	jQuery('form#upload-image').hide();
	
	//アップロードのアコーディオン
	$('.upload').click(function(){
		$('#upload-image').toggle('blind',200);
	})

	//light box
	$('#image-list a').lightBox({
        maxWidth : 800,
        maxHeight: 600
    }); // Select all links in object with gallery ID
})
